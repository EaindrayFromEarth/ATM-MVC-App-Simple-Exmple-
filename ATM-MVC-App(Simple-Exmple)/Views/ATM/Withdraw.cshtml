<!-- Withdraw.cshtml -->
@model CardHolder

<style>
    body {
        margin: 0;
        padding: 0;
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100vh;
        background-image: url('data:image/jpeg;base64,'); /* Add your background image URL here */
        background-size: cover;
    }

    .card {
        background: rgba(255, 255, 255, 0.5); /* Adjust the alpha value for the desired level of transparency */
        border-radius: 10px;
        padding: 20px;
        box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
        text-align: center;
    }

    h2 {
        color: #007BFF; /* Customize the heading color */
    }

    p {
        color: green; /* Customize the current balance color */
        font-size: 24px;
        font-weight: bold;
        margin-bottom: 20px;
    }

    form {
        display: flex;
        flex-direction: column;
        align-items: center;
    }

    label {
        margin-bottom: 10px;
    }

    input {
        width: 100%;
        padding: 10px;
        margin-bottom: 20px;
    }

    button {
        background-color: #DC3545; /* Customize the button color */
        color: #fff;
        border: none;
        padding: 10px 20px;
        border-radius: 5px;
        cursor: pointer;
    }

    .icon {
        margin-right: 10px;
        font-size: 20px;
    }
</style>

<div class="card">
    <h2>Withdrawal</h2>

    <p>Balance: $@Model.Balance</p>

    <form asp-action="Withdraw" asp-route-cardHolderId="@Model.CardHolderId" method="post">
        <label for="amount">Enter Amount:</label>
        <input type="number" id="amount" name="amount" step="0.01" required>
        <button type="submit"><i class="icon fas fa-money-bill-wave"></i> Withdraw</button>
        <a asp-action="MainMenu" asp-route-cardHolderId="@Model.CardHolderId">Back to Main Menu</a>
        <span asp-validation-for="Amount"></span>
    </form>
</div>
